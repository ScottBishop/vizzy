<% if @test_images.size > 0 %>
    <% breadcrumb :successful_tests, @build %>
    <%= render partial: 'layouts/test_images_panel', locals: {title: "#{pluralize(@test_images.size, 'successful test')}, here are the corresponding base images:", images: @test_images, panel_class: "panel-success", panel_id: "successful_images"} %>
<% end %>